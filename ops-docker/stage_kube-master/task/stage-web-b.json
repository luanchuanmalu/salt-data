{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"stage-web-b",
      "labels":{
         "app":"stage-web",
         "role":"groupb"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
        "app":"stage-web",
        "role":"groupb"
      },
      "template":{
         "metadata":{
            "labels":{
              "app":"stage-web",
              "role":"groupb"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"stage-web",
                  "image":"10.170.130.148:5000/keystone/stage-web:latest",
                  "ports":[
                     {
                        "name":"stage-web",
                        "containerPort":8001
                     }
                  ]
               }
            ]
         }
      }
   }
}
